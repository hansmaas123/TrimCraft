// const {locations} = Astro.props
import fetchApi from "../lib/strapi";
const locations = await fetchApi({
  endpoint: "locations",
  wrappedByKey: "data",
  query: { populate: "*"  },
});

const categories = await fetchApi({
  endpoint: "categories",
  wrappedByKey: "data",
});


---

<BaseLayout>
    <LocationList locations = {locations}/>
</BaseLayout>
